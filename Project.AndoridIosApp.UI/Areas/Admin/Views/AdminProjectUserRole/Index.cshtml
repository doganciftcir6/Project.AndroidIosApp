@model List<Project.AndroidIosApp.Dtos.ProjectUserRoleDto.GetProjectUserRoleDto>

@{
    Layout = "_AdminLayout";
}

<!-- table section -->
<div class="col-md-12">
    <div class="white_shd full margin_bottom_30">
        <div class="full graph_head">
            <div class="heading1 margin_0">
                <h2>ProjectUserRoles</h2>
            </div>
        </div>
        <div class="table_section padding_infor_info">
            <a href="/Admin/AdminProjectUserRole/Insert" class="btn btn-outline-info mb-2">Add New ProjectUserRole </a>
            <div class="table-responsive-sm">
                <table class="table table-dark table-striped">
                    <thead>
                        <tr>
                            <th>Id</th>
                            <th>ProjectUserID</th>
                            <th>ProjectUser</th>
                            <th>ProjectRoleID</th>
                            <th>ProjectRole</th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in Model)
                        {
                            <tr>
                                <th>@item.Id</th>
                                <td>@item.ProjectUser.Id</td>
                                <td>@item.ProjectUser.Firstname @item.ProjectUser.Lastname</td>
                                <td>@item.ProjectRole.Id</td>
                                <td>@item.ProjectRole.Definition</td>
                                <td><a href="/Admin/AdminProjectUserRole/Update/@item.Id" class="btn btn-outline-success">Update</a></td>
                                <td><a href="/Admin/AdminProjectUserRole/Delete/@item.Id" class="btn btn-outline-danger">Delete</a></td>
                            </tr>
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>
<!-- table section -->
<!DOCTYPE html>
<html lang="en">
 @*header*@
@await Component.InvokeAsync("AdminHeaderComponent")
<body class="dashboard dashboard_1">
    <div class="full_container">
        <div class="inner_container">
            <!-- Sidebar  -->
            @await Component.InvokeAsync("AdminSideBarComponent")
            <!-- end sidebar -->
            <!-- right content -->
            <div id="content">
                <!-- topbar -->
                @await Component.InvokeAsync("AdminTopBarComponent")
                <!-- end topbar -->
                @RenderBody()
                <!-- footer -->
                @await Component.InvokeAsync("AdminFooterComponent")
            </div>
        </div>
    </div>
    <!-- jQuery -->
    @await Component.InvokeAsync("AdminScriptsComponent")
</body>
</html>